{
    "name": "symfony-console",
    "displayName": "Symfony Console",
    "description": "Use Symfony Console from VS Code Command Palette",
    "version": "0.0.1",
    "publisher": "derappelt",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.runCommand"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.runCommand",
                "title": "Symfony Console"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.21",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "repository": "github:derappelt/vscode-symfony-console"
}
